extends ./layouts/category.pug

block app_variables
  - var pageTitle = "Kashmir Box";
block category_content
  main.category
    //-Product_banner
    .container_fluid
      .row
        .col-md-12.col-lg-12.padding-0.categorybanner
          img(src="https://www.kashmirbox.com/image/catalog/kb1/category/summer-suits.jpg" alt="Banner").img-responsive
          h1 EMBROIDERED ETHNIC KURTIS!
        //-Productlist and filters
        //-will remove with breadcrumb file
        include ./components/breadcrumb.pug
        ol.breadcrumb.productbreadcrumb
